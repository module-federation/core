{
  "extends": "tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@module-federation/enhanced": ["dist/packages/enhanced"],
      "@module-federation/enhanced/*": ["dist/packages/enhanced/*"],
      "@module-federation/sdk": ["dist/packages/sdk/index.esm.js"],
      "@module-federation/sdk/normalize-webpack-path": [
        "dist/packages/sdk/normalize-webpack-path.cjs.js"
      ],
      "@module-federation/webpack-bundler-runtime": [
        "dist/packages/webpack-bundler-runtime/index.esm.js"
      ],
      "@module-federation/runtime": ["dist/packages/runtime/index.esm.js"],
      "@module-federation/runtime/*": ["dist/packages/runtime/*"],
      "@module-federation/core": ["dist/packages/core/src/index.js"],
      "@module-federation/native-federation-tests": [
        "dist/packages/native-federation-tests/src/index.js"
      ],
      "@module-federation/native-federation-typescript": [
        "dist/packages/native-federation-typescript/src/index.js"
      ],
      "@module-federation/nextjs-mf": ["dist/packages/nextjs-mf/src/index.js"],
      "@module-federation/nextjs-mf/*": ["dist/packages/nextjs-mf/*"],
      "@module-federation/node": ["dist/packages/node/src"],
      "@module-federation/node/utils": ["dist/packages/node/src/utils"],
      "@module-federation/storybook-addon": [
        "dist/packages/storybook-addon/src/index.js"
      ],
      "@module-federation/typescript": [
        "dist/packages/typescript/src/index.js"
      ],
      "@module-federation/utilities": ["dist/packages/utilities/src/index.js"],
      "@module-federation/utilities/*": ["dist/packages/utilities/*"]
    }
  }
}
