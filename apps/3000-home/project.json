{
  "name": "3000-home",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/3000-home",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "cache": false,
      "defaultConfiguration": "production",
      "options": {
        "command": "npx next build",
        "cwd": "apps/3000-home"
      },
      "configurations": {
        "development": {},
        "production": {}
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "npx next dev -p 3000",
        "cwd": "apps/3000-home"
      },
      "configurations": {
        "development": {
          "command": "npx next dev -p 3000"
        },
        "production": {
          "command": "npx next start -p 3000"
        }
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec eslint apps/3000-home/**/*.{ts,tsx,js,jsx}",
        "forwardAllArgs": false
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec cypress run --project apps/3000-home --e2e --config baseUrl=http://localhost:3000 --key 27e40c91-5ac3-4433-8a87-651d10f51cf6",
        "forwardAllArgs": false
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/run-next-e2e.mjs --mode=dev"
      },
      "configurations": {
        "production": {
          "command": "node tools/scripts/run-next-e2e.mjs --mode=prod"
        }
      }
    }
  }
}
