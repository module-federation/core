{
  "name": "3000-home",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/3000-home",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "options": {
        "command": "npx next build",
        "cwd": "apps/3000-home"
      },
      "configurations": {
        "development": {},
        "production": {}
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "npx next dev -p 3000",
        "cwd": "apps/3000-home"
      },
      "configurations": {
        "development": {
          "command": "npx next dev -p 3000"
        },
        "production": {
          "command": "npx next start -p 3000"
        }
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/3000-home/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/3000-home/cypress.config.ts",
        "testingType": "e2e",
        "baseUrl": "http://localhost:3000",
        "key": "27e40c91-5ac3-4433-8a87-651d10f51cf6"
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/run-next-e2e.mjs --mode=dev"
      },
      "configurations": {
        "production": {
          "command": "node tools/scripts/run-next-e2e.mjs --mode=prod"
        }
      }
    }
  }
}
