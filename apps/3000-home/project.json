{
  "name": "3000-home",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/3000-home",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "options": {
        "command": "next build",
        "cwd": "apps/3000-home"
      },
      "configurations": {
        "development": {},
        "production": {}
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "next dev -p 3000",
        "cwd": "apps/3000-home",
        "readyWhen": "Ready in"
      },
      "configurations": {
        "development": {},
        "production": {
          "command": "next start -p 3000"
        }
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "3000-home:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/3000-home/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/3000-home/cypress.config.ts",
        "testingType": "e2e",
        "baseUrl": "http://localhost:3000",
        "key": "27e40c91-5ac3-4433-8a87-651d10f51cf6"
      },
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "devServerTarget": "3000-home:serve:development"
        },
        "production": {
          "devServerTarget": "3000-home:serve:production"
        }
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "concurrently --kill-others --success first \"wait-on tcp:3001 tcp:3002 && nx run 3000-home:e2e:development\" \"nx run-many --target=serve --projects=3001-shop,3002-checkout --configuration=development\"",
            "forwardAllArgs": true
          }
        ]
      },
      "configurations": {
        "production": {
          "commands": [
            {
              "command": "concurrently --kill-others --success first \"wait-on tcp:3001 tcp:3002 && NX_TUI=false nx run 3000-home:e2e:production\" \"nx run-many --target=build --projects=3001-shop,3002-checkout --configuration=production --parallel=9 && nx run-many --target=serve --projects=3001-shop,3002-checkout --configuration=production --parallel=9\"",
              "forwardAllArgs": true
            }
          ]
        }
      }
    }
  }
}
