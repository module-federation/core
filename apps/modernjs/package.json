{
  "name": "@module-federation/modernjsapp",
  "private": true,
  "version": "0.1.119",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "serve": "PORT=4001 modern serve",
    "new": "modern new",
    "lint": "modern lint",
    "upgrade": "modern upgrade"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@babel/runtime": "7.28.2",
    "@modern-js/runtime": "2.69.3",
    "@module-federation/enhanced": "workspace:*",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@modern-js-app/eslint-config": "2.59.0",
    "@modern-js/app-tools": "2.69.3",
    "@modern-js/eslint-config": "2.59.0",
    "@modern-js/tsconfig": "2.69.3",
    "@types/jest": "~29.5.0",
    "@types/node": "~20.12.12",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.3.0",
    "lint-staged": "~13.1.0",
    "prettier": "~3.3.3",
    "rimraf": "~6.0.1",
    "typescript": "~5.0.4"
  }
}
