{
  "name": "rstest-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/rstest/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rspack build --config rspack.config.js",
        "cwd": "apps/rstest"
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rstest run",
        "cwd": "apps/rstest"
      }
    },
    "test:node": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rstest run -c rstest.node.config.ts",
        "cwd": "apps/rstest"
      }
    },
    "test-node": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rstest run -c rstest.node.config.ts",
        "cwd": "apps/rstest"
      }
    },
    "test:browser": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rstest run -c rstest.browser.config.ts",
        "cwd": "apps/rstest"
      }
    },
    "test-browser": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rstest run -c rstest.browser.config.ts",
        "cwd": "apps/rstest"
      }
    }
  }
}
