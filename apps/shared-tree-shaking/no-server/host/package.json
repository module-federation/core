{
  "name": "shared-tree-shaking-no-server-host",
  "private": true,
  "version": "1.0.1",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "dev": "modern dev",
    "build": "cd apps/shared-tree-shaking/no-server/host ; pnpm run build",
    "build:re-shake": "SECONDARY_SHARED_TREE_SHAKING=true modern build",
    "start": "modern start",
    "serve": "cd apps/shared-tree-shaking/no-server/host ; pnpm run serve",
    "new": "modern new",
    "lint": "modern lint",
    "upgrade": "modern upgrade",
    "inspect": "modern inspect",
    "e2e": "pnpm exec cypress run --config-file apps/shared-tree-shaking/no-server/host/cypress.config.ts --browser chrome --config baseUrl=http://localhost:3001",
    "e2e:development": "pnpm exec cypress open --config-file apps/shared-tree-shaking/no-server/host/cypress.config.ts --browser chrome --config baseUrl=http://localhost:3001",
    "test:e2e": "pnpm --filter shared-tree-shaking-no-server-host run build && pnpm --filter shared-tree-shaking-no-server-provider run build && pnpm --filter shared-tree-shaking-no-server-host run serve & pnpm --filter shared-tree-shaking-no-server-provider run serve & sleep 10 && pnpm --filter shared-tree-shaking-no-server-host run e2e"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@babel/runtime": "7.28.2",
    "@modern-js/runtime": "3.0.1",
    "@module-federation/enhanced": "workspace:*",
    "antd": "6.0.1",
    "react": "~18.3.1",
    "react-dom": "~18.3.1"
  },
  "devDependencies": {
    "@modern-js-app/eslint-config": "2.59.0",
    "@modern-js/app-tools": "3.0.1",
    "@modern-js/tsconfig": "3.0.1",
    "@modern-js/server-runtime": "3.0.1",
    "@modern-js/plugin-server": "2.68.0",
    "@types/jest": "~29.5.0",
    "@types/node": "^20.19.5",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.3.0",
    "lint-staged": "~13.1.0",
    "prettier": "~3.3.3",
    "rimraf": "~3.0.2",
    "typescript": "~5.0.4",
    "fs-extra": "^11.1.1",
    "ts-node": "~10.8.1",
    "tsconfig-paths": "~3.14.1"
  }
}
