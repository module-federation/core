{
  "name": "shared-tree-shaking-with-server-host",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shared-tree-shaking/with-server/with-server/host/src",
  "projectType": "application",
  "tags": [],
  "implicitDependencies": ["typescript"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd apps/shared-tree-shaking/with-server/host; pnpm run build",
            "forwardAllArgs": true
          }
        ]
      }
    },
    "build:re-shake": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd apps/shared-tree-shaking/with-server/host; pnpm run build:re-shake",
            "forwardAllArgs": true
          }
        ]
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd apps/shared-tree-shaking/with-server/host; pnpm run serve",
            "forwardAllArgs": true
          }
        ]
      }
    },
    "serve:re-shake": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd apps/shared-tree-shaking/with-server/host; pnpm run serve:re-shake",
            "forwardAllArgs": true
          }
        ]
      }
    },
    "serve:all": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run shared-tree-shaking-with-server-host:build",
            "forwardAllArgs": false
          },
          {
            "command": "nx run shared-tree-shaking-with-server-host:build:re-shake",
            "forwardAllArgs": false
          },
          {
            "command": "nx run shared-tree-shaking-with-server-provider:build",
            "forwardAllArgs": false
          },
          {
            "command": "nx run shared-tree-shaking-with-server-host:serve & nx run shared-tree-shaking-with-server-provider:serve & nx run shared-tree-shaking-with-server-host:serve:re-shake",
            "forwardAllArgs": false
          }
        ]
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec cypress run --config-file apps/shared-tree-shaking/with-server/host/cypress.config.ts --browser chrome --config baseUrl=http://localhost:3001",
        "forwardAllArgs": false
      },
      "configurations": {
        "development": {
          "command": "pnpm exec cypress open --config-file apps/shared-tree-shaking/with-server/host/cypress.config.ts --browser chrome --config baseUrl=http://localhost:3001"
        }
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run shared-tree-shaking-with-server-host:build",
            "forwardAllArgs": false
          },
          {
            "command": "nx run shared-tree-shaking-with-server-host:build:re-shake",
            "forwardAllArgs": false
          },
          {
            "command": "nx run shared-tree-shaking-with-server-provider:build",
            "forwardAllArgs": false
          },
          {
            "command": "nx run shared-tree-shaking-with-server-host:serve & nx run shared-tree-shaking-with-server-provider:serve & nx run shared-tree-shaking-with-server-host:serve:re-shake & sleep 10 && nx run shared-tree-shaking-with-server-host:e2e",
            "forwardAllArgs": false
          }
        ]
      }
    }
  }
}
