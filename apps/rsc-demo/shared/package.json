{
  "name": "@rsc-demo/shared",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "rsc-demo": "./src/index.js",
      "require": "./src/index.js",
      "import": "./src/index.js",
      "default": "./dist/index.js"
    },
    "./shared-server-actions": {
