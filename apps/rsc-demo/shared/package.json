    "react-dom": "19.2.0",
    "@module-federation/react-server-dom-webpack": "workspace:*",
    "sanitize-html": "^2.10.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@module-federation/rsc": "workspace:*",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rslib/core": "^0.12.4"
