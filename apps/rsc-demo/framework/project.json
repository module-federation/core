{
  "name": "rsc-demo-framework",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/rsc-demo/framework",
  "projectType": "library",
  "tags": ["rsc", "demo", "type:pkg"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ],
      "options": {
        "cwd": "apps/rsc-demo/framework",
        "command": "pnpm run build"
      }
    }
  }
}
