{
  "name": "@module-federation/3001-shop",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.0",
    "antd": "5.19.1",
    "lodash": "4.17.23",
    "next": "14.2.35",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@module-federation/nextjs-mf": "workspace:*",
    "@module-federation/runtime": "workspace:*",
    "@module-federation/sdk": "workspace:*",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "webpack": "5.104.1"
  },
  "scripts": {
    "start": "next start",
    "build": "NEXT_PRIVATE_LOCAL_WEBPACK=true next build",
    "build:development": "npx next build",
    "build:production": "npx next build",
    "serve": "npx next dev -p 3001",
    "serve:development": "npx next dev -p 3001",
    "serve:production": "npx next start -p 3001",
    "lint": "ESLINT_USE_FLAT_CONFIG=false pnpm exec eslint --ignore-pattern node_modules **/*.{ts,tsx,js,jsx}",
    "test:e2e": "node tools/scripts/run-next-e2e.mjs --mode=dev",
    "test:e2e:production": "node tools/scripts/run-next-e2e.mjs --mode=prod",
    "e2e": "pnpm exec cypress run --project . --e2e --config baseUrl=http://localhost:3001"
  }
}
