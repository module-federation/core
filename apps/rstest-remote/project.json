{
  "name": "rstest-remote-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/rstest-remote/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rspack build --config rspack.config.js",
        "cwd": "apps/rstest-remote"
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "build:node": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rspack build --config rspack.node.config.js",
        "cwd": "apps/rstest-remote"
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rspack serve --config rspack.config.js --host 127.0.0.1 --port 3016",
        "cwd": "apps/rstest-remote"
      }
    }
  }
}
