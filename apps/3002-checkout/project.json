{
  "name": "3002-checkout",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/3002-checkout",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "options": {
        "command": "npx next build",
        "cwd": "apps/3002-checkout"
      },
      "configurations": {
        "development": {},
        "production": {}
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "npx next dev -p 3002",
        "cwd": "apps/3002-checkout"
      },
      "configurations": {
        "development": {
          "command": "npx next dev -p 3002"
        },
        "production": {
          "command": "npx next start -p 3002"
        }
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/3002-checkout/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/3002-checkout/cypress.config.ts",
        "testingType": "e2e",
        "baseUrl": "http://localhost:3002"
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "npx kill-port 3000 3001 3002",
            "forwardAllArgs": false
          },
          {
            "command": "NX_TUI=false nx run-many --target=serve --projects=3000-home,3001-shop,3002-checkout --configuration=development & wait-on tcp:3000 tcp:3001 tcp:3002",
            "forwardAllArgs": false
          },
          {
            "command": "wait-on tcp:3000 tcp:3001 tcp:3002 && NX_TUI=false nx run 3002-checkout:e2e",
            "forwardAllArgs": true
          },
          {
            "command": "npx kill-port 3000 3001 3002",
            "forwardAllArgs": false
          }
        ]
      },
      "configurations": {
        "production": {
          "parallel": false,
          "commands": [
            {
              "command": "npx kill-port 3000 3001 3002",
              "forwardAllArgs": false
            },
            {
              "command": "NX_TUI=false nx run-many --target=serve --projects=3000-home,3001-shop,3002-checkout --configuration=production --parallel=9 & wait-on tcp:3000 tcp:3001 tcp:3002",
              "forwardAllArgs": false
            },
            {
              "command": "wait-on tcp:3000 tcp:3001 tcp:3002 && NX_TUI=false nx run 3002-checkout:e2e",
              "forwardAllArgs": true
            },
            {
              "command": "npx kill-port 3000 3001 3002",
              "forwardAllArgs": false
            }
          ]
        }
      }
    }
  }
}
