{
  "name": "3002-checkout",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/3002-checkout",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "production",
      "options": {
        "command": "next build",
        "cwd": "apps/3002-checkout"
      },
      "configurations": {
        "development": {},
        "production": {}
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "next dev -p 3002",
        "cwd": "apps/3002-checkout",
        "readyWhen": "Ready in"
      },
      "configurations": {
        "development": {},
        "production": {
          "command": "next start -p 3002"
        }
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "3002-checkout:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/3002-checkout/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/3002-checkout/cypress.config.ts",
        "testingType": "e2e",
        "baseUrl": "http://localhost:3002"
      },
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "devServerTarget": "3002-checkout:serve:development"
        },
        "production": {
          "devServerTarget": "3002-checkout:serve:production"
        }
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "concurrently --kill-others --success first \"wait-on tcp:3000 tcp:3001 && nx run 3002-checkout:e2e:development\" \"nx run-many --target=serve --projects=3000-home,3001-shop --configuration=development\"",
            "forwardAllArgs": true
          }
        ]
      },
      "configurations": {
        "production": {
          "commands": [
            {
              "command": "concurrently --kill-others --success first \"wait-on tcp:3000 tcp:3001 && NX_TUI=false nx run 3002-checkout:e2e:production\" \"nx run-many --target=build --projects=3000-home,3001-shop --configuration=production --parallel=9 && nx run-many --target=serve --projects=3000-home,3001-shop --configuration=production --parallel=9\"",
              "forwardAllArgs": true
            }
          ]
        }
      }
    }
  }
}
