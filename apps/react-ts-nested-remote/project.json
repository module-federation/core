{
  "name": "react_ts_nested_remote",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/react-ts-nested-remote/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["apps/react-ts-nested-remote/dist"],
      "defaultConfiguration": "production",
      "options": {
        "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli --config apps/react-ts-nested-remote/webpack.config.js --mode production",
        "forwardAllArgs": false
      },
      "configurations": {
        "development": {
          "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli --config apps/react-ts-nested-remote/webpack.config.js --mode development"
        },
        "production": {
          "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli --config apps/react-ts-nested-remote/webpack.config.js --mode production"
        }
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli serve --config apps/react-ts-nested-remote/webpack.config.js --mode development --port 3005",
        "forwardAllArgs": false
      },
      "configurations": {
        "development": {
          "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli serve --config apps/react-ts-nested-remote/webpack.config.js --mode development --port 3005"
        },
        "production": {
          "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli serve --config apps/react-ts-nested-remote/webpack.config.js --mode production --port 3005 --no-hot"
        }
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec eslint apps/react-ts-nested-remote/**/*.{ts,tsx,js,jsx}",
        "forwardAllArgs": false
      }
    },
    "serve-static": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "pnpm exec serve apps/react-ts-nested-remote/dist -l 3005 --cors",
        "forwardAllArgs": false
      }
    }
  }
}
