{
  "name": "router-host-v5-2200",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/router-demo/router-host-v5-2200/src",
  "projectType": "library",
  "tags": ["type:app"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npm run build --prefix apps/router-demo/router-host-v5-2200"
        ]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npm run test --prefix apps/router-demo/router-host-v5-2200"
        ]
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npm run dev --prefix apps/router-demo/router-host-v5-2200"
        ]
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec cypress run --config-file apps/router-demo/router-host-v5-2200/cypress.config.ts --e2e --config baseUrl=http://127.0.0.1:2000 --browser chrome",
        "forwardAllArgs": false
      },
      "configurations": {
        "development": {
          "command": "pnpm exec cypress open --config-file apps/router-demo/router-host-v5-2200/cypress.config.ts --e2e --config baseUrl=http://127.0.0.1:2000 --browser electron"
        }
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": true,
        "commands": [
          {
            "command": "lsof -i :3005 || nx run 3005-runtime-host:serve",
            "forwardAllArgs": false
          },
          {
            "command": "sleep 4 && nx run 3005-runtime-host:e2e",
            "forwardAllArgs": true
          }
        ]
      }
    }
  }
}
