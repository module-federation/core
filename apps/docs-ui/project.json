{
  "name": "docs-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs-ui/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["apps/docs-ui/dist"],
      "defaultConfiguration": "production",
      "options": {
        "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli --config apps/docs-ui/webpack.config.js --mode production",
        "forwardAllArgs": false
      },
      "configurations": {
        "production": {
          "command": "NODE_OPTIONS=--max_old_space_size=4096 pnpm exec webpack-cli --config apps/docs-ui/webpack.config.js --mode production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/docs-ui/**/*.ts"]
      }
    }
  }
}
