  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run start:next",
      "name": "Run npm start:next",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "gpt:pr",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/gpt/index.js",
      "args": ["--pr"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "gpt:commit",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/gpt/index.js",
      "args": ["--commit"]
    }
  ]
}
