{
  "name": "managers",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/managers/src",
  "projectType": "library",
  "tags": ["type:pkg"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/packages/managers/dist"],
      "options": {
        "command": "rslib build",
        "cwd": "packages/managers"
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/managers/**/*.ts",
          "packages/managers/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/managers/jest.config.js",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "pre-release": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run managers:test",
            "forwardAllArgs": false
          },
          {
            "command": "nx run managers:build",
            "forwardAllArgs": false
          }
        ]
      }
    }
  }
}
