{
  "name": "@module-federation/runtime-core",
  "version": "0.14.0",
  "type": "module",
  "author": "zhouxiao <codingzx@gmail.com>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/module-federation/core/",
    "directory": "packages/runtime-core"
  },
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "postbuild": "npx tsc --declaration --declarationDir ./dist --emitDeclarationOnly --project tsconfig.lib.json && find dist -name 'index.d.ts' -path '*/src/index.d.ts' -exec cp {} dist/index.d.ts \\; && find dist -name 'types.d.ts' -path '*/src/types.d.ts' -exec cp {} dist/types.d.ts \\;"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "dependencies": {
    "@module-federation/sdk": "workspace:*",
    "@module-federation/error-codes": "workspace:*"
  }
}
