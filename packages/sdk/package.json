{
  "name": "@module-federation/sdk",
  "version": "2.0.1",
  "type": "module",
  "license": "MIT",
  "description": "A sdk for support module federation",
  "keywords": [
    "Module Federation",
    "sdk"
  ],
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/module-federation/core.git",
    "directory": "packages/sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "zhanghang <hanric.zhang@gmail.com>",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {
    "url": false
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./normalize-webpack-path": {
      "import": {
        "types": "./dist/normalize-webpack-path.d.ts",
        "default": "./dist/normalize-webpack-path.js"
      },
      "require": {
        "types": "./dist/normalize-webpack-path.d.ts",
        "default": "./dist/normalize-webpack-path.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "normalize-webpack-path": [
        "./dist/normalize-webpack-path.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "lint": "ESLINT_USE_FLAT_CONFIG=false pnpm exec eslint --ignore-pattern node_modules \"**/*.ts\" \"package.json\"",
    "test": "pnpm exec jest --config jest.config.cjs --passWithNoTests",
    "test:ci": "pnpm exec jest --config jest.config.cjs --passWithNoTests --ci --coverage",
    "pre-release": "pnpm --filter @module-federation/sdk run test && pnpm --filter @module-federation/sdk run build",
    "turbo:build": "pnpm run build",
    "turbo:test": "pnpm run test",
    "turbo:lint": "pnpm run lint"
  }
}
