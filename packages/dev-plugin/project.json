{
  "name": "dev-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/dev-plugin/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/packages/dev-plugin/dist"],
      "options": {
        "parallel": false,
        "commands": [
          "tsup --config packages/dev-plugin/tsup.config.ts",
          "cp packages/dev-plugin/package.json packages/dev-plugin/dist",
          "cp packages/dev-plugin/*.md packages/dev-plugin/dist"
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/dev-plugin/**/*.ts"]
      }
    },
    "pre-release": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run dev-plugin:test",
            "forwardAllArgs": false
          },
          {
            "command": "nx run dev-plugin:build",
            "forwardAllArgs": false
          }
        ]
      }
    },
    "semantic-release": {
      "executor": "@goestav/nx-semantic-release:semantic-release"
    }
  },
  "tags": ["package"],
  "implicitDependencies": []
}
