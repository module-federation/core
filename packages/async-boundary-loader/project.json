{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/async-boundary-loader/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/async-boundary-loader",
        "main": "packages/async-boundary-loader/src/index.ts",
        "tsConfig": "packages/async-boundary-loader/tsconfig.lib.json",
        "assets": ["packages/async-boundary-loader/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/async-boundary-loader/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/async-boundary-loader"],
      "options": {
        "jestConfig": "packages/async-boundary-loader/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
