{
  "name": "@module-federation/rsbuild-plugin",
  "version": "2.0.1",
  "description": "Module Federation plugin for Rsbuild",
  "homepage": "https://module-federation.io",
  "bugs": {
    "url": "https://github.com/module-federation/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/module-federation/core.git",
    "directory": "packages/rsbuild-plugin"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/cli/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/cli/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils/index.d.mts",
        "default": "./dist/utils.mjs"
      },
      "require": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils.js"
      }
    },
    "./constant": {
      "import": {
        "types": "./dist/constant.d.mts",
        "default": "./dist/constant.mjs"
      },
      "require": {
        "types": "./dist/constant.d.ts",
        "default": "./dist/constant.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/cli/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/cli/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "constant": [
        "./dist/constant.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "fs-extra": "11.3.0",
    "@module-federation/sdk": "workspace:*",
    "@module-federation/enhanced": "workspace:*",
    "@module-federation/node": "workspace:*"
  },
  "devDependencies": {
    "@rslib/core": "^0.19.6",
    "@rsbuild/core": "2.0.0-beta.4"
  },
  "peerDependencies": {
    "@rsbuild/core": "2.0.0-beta.4"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
