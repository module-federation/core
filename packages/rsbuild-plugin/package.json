{
  "name": "@module-federation/rsbuild-plugin",
  "version": "0.16.0",
  "description": "Module Federation plugin for Rsbuild",
  "homepage": "https://module-federation.io",
  "type": "module",
  "bugs": {
    "url": "https://github.com/module-federation/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/module-federation/core",
    "directory": "packages/rsbuild-plugin"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./constant": {
      "types": "./dist/constant.d.ts",
      "import": "./dist/constant.js",
      "require": "./dist/constant.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/cli/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/cli/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "constant": [
        "./dist/constant.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "fs-extra": "11.3.0",
    "@module-federation/sdk": "workspace:*",
    "@module-federation/enhanced": "workspace:*",
    "@module-federation/node": "workspace:*"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.3.21"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.3.21"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
