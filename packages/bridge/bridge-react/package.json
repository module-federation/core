      ".": [
        "./dist/index.d.ts"
      ],
      "base": [
        "./dist/base.d.ts"
      ],
      "v18": [
        "./dist/v18.d.ts"
      ],
      "v19": [
        "./dist/v19.d.ts"
      ],
      "router": [
        "./dist/router.d.ts"
      ],
      "plugin": [
        "./dist/plugin.d.ts"
      ],
      "router-v5": [
        "./dist/router-v5.d.ts"
