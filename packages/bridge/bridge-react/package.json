    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.22.3",
    "react-router": "7.9.4",
    "typescript": "^5.2.2",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.3.0",
    "@module-federation/runtime": "workspace:*",
    "@module-federation/runtime-core": "workspace:*",
    "hono": "4.10.3"
  }
}
