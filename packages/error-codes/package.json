{
  "name": "@module-federation/error-codes",
  "description": "Module Federation Error Codes",
  "author": "zhanghang <hanric.zhang@gmail.com>",
  "public": true,
  "sideEffects": false,
  "version": "2.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/module-federation/core.git",
    "directory": "packages/error-codes"
  },
  "keywords": [
    "Module Federation",
    "error codes"
  ],
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "url": false
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.esm.mjs",
      "require": "./dist/browser.cjs.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.esm.mjs",
      "require": "./dist/node.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "browser": [
        "./dist/browser.d.ts"
      ],
      "node": [
        "./dist/node.d.ts"
      ]
    }
  }
}
