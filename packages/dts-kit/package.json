{
  "name": "@module-federation/dts-kit",
  "version": "0.0.17",
  "description": "Bundler federated types",
  "keywords": [
    "module federation",
    "typescript",
    "remote types",
    "federated types"
  ],
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Matteo Pietro Dazzi <matteopietro.dazzi@gmail.com> (https://github.com/ilteoood)",
  "contributors": [
    "zhanghang <hanric.zhang@gmail.com> https://github.com/2heal1"
  ],
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "ansi-colors": "^4.1.3",
    "axios": "^1.6.7",
    "rambda": "^9.1.0",
    "fs-extra": "9.1.0",
    "@module-federation/dev-server": "workspace:*",
    "@module-federation/sdk": "workspace:*",
    "@module-federation/managers": "workspace:*",
    "lodash.clonedeepwith": "4.5.0",
    "@module-federation/third-party-dts-extractor": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^4.9.0 || ^5.0.0",
    "vue-tsc": "^1.0.24"
  }
}
