{
  "name": "@module-federation/native-federation-typescript",
  "version": "0.6.2",
  "description": "Bundler agnostic unplugin to share federated types",
  "keywords": [
    "module federation",
    "typescript",
    "remote types",
    "federated types"
  ],
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./rollup": {
      "require": {
        "types": "./dist/rollup.d.ts",
        "default": "./dist/rollup.js"
      },
      "import": {
        "types": "./dist/rollup.d.mts",
        "default": "./dist/rollup.mjs"
      }
    },
    "./vite": {
      "require": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "import": {
        "types": "./dist/vite.d.mts",
        "default": "./dist/vite.mjs"
      }
    },
    "./webpack": {
      "require": {
        "types": "./dist/webpack.d.ts",
        "default": "./dist/webpack.js"
      },
      "import": {
        "types": "./dist/webpack.d.mts",
        "default": "./dist/webpack.mjs"
      }
    },
    "./esbuild": {
      "require": {
        "types": "./dist/esbuild.d.ts",
        "default": "./dist/esbuild.js"
      },
      "import": {
        "types": "./dist/esbuild.d.mts",
        "default": "./dist/esbuild.mjs"
      }
    },
    "./rspack": {
      "require": {
        "types": "./dist/rspack.d.ts",
        "default": "./dist/rspack.js"
      },
      "import": {
        "types": "./dist/rspack.d.mts",
        "default": "./dist/rspack.mjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "Matteo Pietro Dazzi <matteopietro.dazzi@gmail.com> (https://github.com/ilteoood)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/module-federation/core.git",
    "directory": "packages/native-federation-typescript"
  },
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.14",
    "ansi-colors": "^4.1.3",
    "axios": "^1.12.0",
    "rambda": "^9.2.1",
    "unplugin": "^1.10.1"
  },
  "peerDependencies": {
    "typescript": "^4.9.0 || ^5.0.0",
    "vue-tsc": "^1.0.24"
  },
  "peerDependenciesMeta": {
    "vue-tsc": {
      "optional": true
    }
  },
  "devDependencies": {
    "upath": "2.0.1"
  }
}
