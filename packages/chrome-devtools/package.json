{
  "name": "@module-federation/devtools",
  "version": "0.22.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/module-federation/core.git",
    "directory": "packages/chrome-devtools"
  },
  "scripts": {
    "build:storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "reset": "npx rimraf ./**/node_modules",
    "dev": "modern-app dev",
    "build": "modern-app build && node postpack.js",
    "build:debug": "DEBUG=true modern-app build && node postpack.js",
    "build:lib": "rm -rf dist && modern-module build -c modern.lib.config.ts",
    "release": "npm publish --tag canary",
    "test": "vitest run",
    "test:e2e": "E2ETEST=true pnpm build && playwright test",
    "test:e2e:ui": "E2ETEST=true  pnpm build && playwright test --ui",
    "start": "modern-app start",
    "serve": "modern-app serve",
    "postinstall": "bash install-deps.bash"
  },
  "files": [
    "dist/",
    "install-deps.bash",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/App.d.ts",
      "require": "./dist/lib/App.js",
      "import": "./dist/es/App.js"
    },
    "./App": {
      "types": "./dist/types/App.d.ts",
      "require": "./dist/lib/App.js",
      "import": "./dist/es/App.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "types": "./dist/index.d.ts"
  },
  "dependencies": {
    "@modern-js/runtime": "2.69.3",
    "@arco-design/web-react": "2.66.7",
    "@module-federation/sdk": "workspace:*",
    "ahooks": "^3.7.10",
    "dagre": "^0.8.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reactflow": "11.11.4",
    "echarts": "^6.0.0",
    "lucide-react": "^0.364.0",
    "echarts-for-react": "^3.0.5"
  },
  "devDependencies": {
    "@modern-js-app/eslint-config": "2.59.0",
    "@modern-js/app-tools": "2.69.3",
    "@modern-js/eslint-config": "2.59.0",
    "@modern-js/module-tools": "2.69.3",
    "@modern-js/storybook": "2.69.3",
    "@modern-js/tsconfig": "2.69.3",
    "@module-federation/runtime": "workspace:*",
    "@playwright/test": "1.49.1",
    "@types/chrome": "^0.0.272",
    "@types/dagre": "^0.7.52",
    "@types/jest": "~29.2.4",
    "@types/node": "~20.12.12",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "lint-staged": "~13.1.0",
    "prettier": "~3.3.3",
    "rimraf": "~6.0.1",
    "vitest": "1.2.2"
  }
}
