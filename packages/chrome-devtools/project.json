{
  "name": "chrome-devtools",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/chrome-devtools/src",
  "projectType": "library",
  "targets": {
    "build": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["pnpm build"],
        "cwd": "packages/chrome-devtools"
      }
    },
    "lint": {
      "eslintConfig": "packages/chrome-devtools/.eslintrc.js",
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/chrome-devtools/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{workspaceRoot}/coverage/packages/chrome-devtools"],
      "options": {
        "jestConfig": "packages/chrome-devtools/vitest.config.ts",
        "passWithNoTests": true
      }
    },
    "pre-release": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run chrome-devtools:test",
            "forwardAllArgs": false
          },
          {
            "command": "nx run chrome-devtools:build",
            "forwardAllArgs": false
          }
        ]
      }
    }
  },
  "tags": ["package"]
}
