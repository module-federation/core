{
  "public": true,
  "name": "@module-federation/node",
  "version": "2.7.32",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json",
    "./runtimePlugin": {
      "import": "./dist/cjs/runtimePlugin.js",
      "require": "./dist/cjs/runtimePlugin.js"
    },
    "./record-dynamic-remote-entry-hash-plugin": {
      "import": "./dist/cjs/recordDynamicRemoteEntryHashPlugin.js",
      "require": "./dist/cjs/recordDynamicRemoteEntryHashPlugin.js"
    },
    "./utils": {
      "import": "./dist/cjs/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    },
    "./src/plugins/NodeFederationPlugin.js": {
      "import": "./dist/cjs/plugins/NodeFederationPlugin.js",
      "require": "./dist/cjs/plugins/NodeFederationPlugin.js"
    },
    "./universe-entry-chunk-tracker-plugin": {
      "import": "./dist/cjs/plugins/UniverseEntryChunkTrackerPlugin.js",
      "require": "./dist/cjs/plugins/UniverseEntryChunkTrackerPlugin.js"
    },
    "./src/*": {
      "import": "./dist/cjs/*",
      "require": "./dist/cjs/*"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/src/index.d.ts"
      ],
      "runtimePlugin": [
        "./dist/src/runtimePlugin.d.ts"
      ],
      "utils": [
        "./dist/src/utils/index.d.ts"
      ],
      "record-dynamic-remote-entry-hash-plugin": [
        "./dist/src/recordDynamicRemoteEntryHashPlugin.d.ts"
      ],
      "universe-entry-chunk-tracker-plugin": [
        "./dist/src/plugins/UniverseEntryChunkTrackerPlugin.d.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "tags": [
    "module-federation",
    "node",
    "webpack",
    "node-federation",
    "node-module-federation",
    "ssr"
  ],
  "types": "./dist/src/index.d.ts",
  "description": "Module Federation helper for Node",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/module-federation/core.git",
    "directory": "packages/node"
  },
  "author": "Zack Jackson <zackary.l.jackson@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "btoa": "1.2.1",
    "encoding": "^0.1.13",
    "node-fetch": "2.7.0",
    "@module-federation/enhanced": "workspace:*",
    "@module-federation/sdk": "workspace:*",
    "@module-federation/runtime": "workspace:*"
  },
  "peerDependencies": {
    "webpack": "^5.40.0"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    }
  }
}
