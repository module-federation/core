{
  "name": "utils",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/utilities/src",
  "projectType": "library",
  "tags": ["type:pkg"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/packages/utilities/dist"],
      "options": {
        "command": "rslib build",
        "cwd": "packages/utilities"
      },
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "lint": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputFile}"],
      "options": {
        "command": "pnpm exec eslint \"packages/utilities/**/*.ts\"",
        "forwardAllArgs": false
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/packages/utilities"],
      "options": {
        "command": "pnpm exec jest --config packages/utilities/jest.config.ts --passWithNoTests",
        "forwardAllArgs": false
      }
    },
    "pre-release": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run utils:test",
            "forwardAllArgs": false
          },
          {
            "command": "nx run utils:build",
            "forwardAllArgs": false
          }
        ]
      }
    }
  }
}
