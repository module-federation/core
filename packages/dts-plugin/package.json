{
  "name": "@module-federation/dts-plugin",
  "version": "2.0.1",
  "author": "hanric <hanric.zhang@gmail.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/module-federation/core.git",
    "directory": "packages/dts-plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/core/index.d.mts",
        "default": "./dist/core.mjs"
      },
      "require": {
        "types": "./dist/core/index.d.ts",
        "default": "./dist/core.js"
      }
    },
    "./dynamic-remote-type-hints-plugin": {
      "import": {
        "types": "./dist/runtime-plugins/dynamic-remote-type-hints-plugin.d.mts",
        "default": "./dist/dynamic-remote-type-hints-plugin.mjs"
      },
      "require": {
        "types": "./dist/runtime-plugins/dynamic-remote-type-hints-plugin.d.ts",
        "default": "./dist/dynamic-remote-type-hints-plugin.js"
      }
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "core": [
        "./dist/core/index.d.ts"
      ],
      "dynamic-remote-type-hints-plugin": [
        "./dist/runtime-plugins/dynamic-remote-type-hints-plugin.d.ts"
      ]
    }
  },
  "dependencies": {
    "@module-federation/error-codes": "workspace:*",
    "@module-federation/managers": "workspace:*",
    "@module-federation/sdk": "workspace:*",
    "@module-federation/third-party-dts-extractor": "workspace:*",
    "adm-zip": "^0.5.10",
    "ansi-colors": "^4.1.3",
    "axios": "^1.12.0",
    "chalk": "3.0.0",
    "fs-extra": "9.1.0",
    "isomorphic-ws": "5.0.0",
    "lodash.clonedeepwith": "4.5.0",
    "log4js": "6.9.1",
    "node-schedule": "2.1.1",
    "rambda": "^9.1.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@module-federation/runtime": "workspace:*",
    "@types/node-schedule": "2.1.7",
    "@types/ws": "8.5.12",
    "@vue/tsconfig": "^0.7.0",
    "rimraf": "~6.0.1",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.10",
    "webpack": "^5.104.1"
  },
  "peerDependencies": {
    "typescript": "^4.9.0 || ^5.0.0",
    "vue-tsc": ">=1.0.24"
  },
  "peerDependenciesMeta": {
    "vue-tsc": {
      "optional": true
    }
  }
}
