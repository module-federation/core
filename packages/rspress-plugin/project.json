{
  "name": "rspress-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/rspress-plugin/src",
  "projectType": "library",
  "tags": ["type:pkg"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm -C packages/rspress-plugin build"]
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": ["error-codes"]
        },
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm -C packages/rspress-plugin dev"]
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": ["error-codes"]
        },
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "pre-release": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          {
            "command": "nx run webpack-bundler-runtime:build",
            "forwardAllArgs": false
          }
        ]
      }
    }
  }
}
