{
  "name": "module-federation",
  "version": "0.0.0",
  "private": true,
  "description": "Module Federation helper for NextJS",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": "https://github.com/module-federation/nextjs-mf",
  "author": "Zack Jackson <zackary.l.jackson@gmail.com>",
  "contributors": [
    "Pavel Chertorogov, nodkz <pavel.chertorogov@gmail.com> (www.ps.kz)",
    "Kyle Tsang (https://github.com/kyletsang)"
  ],
  "license": "MIT",
  "scripts": {
    "link:libs": "concurrently 'cd dist/libs/nextjs-mf && npm link' 'cd dist/apps/3000-home && npm link @module-federation/nextjs-mf' 'cd dist/apps/3000-home && npm link @module-federation/node'",
    "build": "yarn build:libs && nx run-many --target=build --projects=3000-home --verbose --output-style=stream",
    "oldbuuild": "nx build",
    "build:libs": "nx run-many --target=build --projects=utils,node,nextjs-mf --verbose --output-style=stream",
    "lint": "nx run-many --target=lint",
    "test": "nx run-many --target=test",
    "start:next": "yarn build:libs && concurrently 'nx run 3000-home:serve:development --output-style=stream' 'nx run 3001-shop:serve:development --output-style=stream' 'nx run 3002-checkout:serve:development --output-style=stream'",
    "serve:next": "yarn build:libs && concurrently 'nx run 3000-home:serve:production --output-style=stream' 'nx run 3001-shop:serve:production --output-style=stream' 'nx run 3002-checkout:serve:production --output-style=stream'",
    "prod": "nx build:libs && nx run-many --target=build --verbose --output-style=stream",
    "commit": "cz",
    "release:node": "nx run node:version --releaseAs=minor",
    "release:next": "nx run nextjs-mf:version --releaseAs=patch",
    "release:utils": "nx run utils:version --releaseAs=minor",
    "prerelease:node": "nx run node:version --releaseAs=prerelease --preid=beta",
    "prerelease:next": "nx run nextjs-mf:version --releaseAs=prerelease --preid=beta",
    "prerelease:utils": "nx run utils:version --releaseAs=prerelease --preid=beta",
    "release:utils:manual": "nx build && cd dist/packages/utilities && yarn publish --non-interactive --access public",
    "release:next:manual": "nx build && cd dist/packages/nextjs-mf && yarn publish --non-interactive --access public",
    "release:node:manual": "nx build && cd dist/packages/node && yarn publish --non-interactive --access public"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "ansi-colors": "^4.1.3",
    "axios": "^1.0.0",
    "core-js": "^3.6.5",
    "fast-glob": "^3.2.12",
    "lodash.get": "^4.4.2",
    "next": "13.3.0",
    "rambda": "^7.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.10.0",
    "regenerator-runtime": "0.13.7",
    "tsup": "^6.7.0",
    "unplugin": "^1.3.1"
  },
