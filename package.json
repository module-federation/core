    "swc-loader": "0.2.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "tsup": "^6.7.0",
    "typescript": "5.0.4",
    "verdaccio": "5.24.0",
