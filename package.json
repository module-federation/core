    "prettier": "3.3.3",
    "prettier-eslint": "16.3.0",
    "prettier-plugin-tailwindcss": "0.5.14",
    "publint": "^0.3.13",
    "react-refresh": "0.14.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-copy": "3.5.0",
    "rsbuild-plugin-publint": "^0.2.1",
