    "@antora/cli": "^3.1.0",
    "@antora/lunr-extension": "^1.0.0-alpha.8",
    "@antora/site-generator": "^3.1.0",
    "@asciidoctor/core": "~2.2",
    "@asciidoctor/tabs": "^1.0.0-alpha.12",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@fontsource/roboto": "4.5.8",
    "@fontsource/roboto-mono": "4.5.10",
    "@jscutlery/semver": "^2.27.1",
    "@node-rs/jieba": "^1.6.2",
    "@nrwl/cli": "15.9.2",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/jest": "15.9.2",
    "@nrwl/js": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/next": "15.9.2",
    "@nrwl/react": "15.9.2",
    "@nrwl/vite": "15.9.2",
    "@nrwl/web": "15.9.2",
    "@nrwl/webpack": "15.9.2",
    "@nrwl/workspace": "15.9.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.3.3",
    "@swc/helpers": "~0.4.0",
    "@swc/jest": "0.2.26",
    "@testing-library/react": "14.0.0",
    "@types/adm-zip": "^0.5.0",
    "@types/download": "^8.0.1",
