    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jiti": "2.4.2",
    "js-yaml": "4.1.0",
    "jsonc-eslint-parser": "^3.1.0",
    "kill-port": "^2.0.1",
    "mime-types": "2.1.35",
